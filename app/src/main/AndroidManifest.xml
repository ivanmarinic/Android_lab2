<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.tvz.android.listamarinic">

    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_cryptoapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cryptoapp_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ListaMarinic">
        <activity android:name=".ThirdActivity" android:parentActivityName=".SecondActivity"></activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>